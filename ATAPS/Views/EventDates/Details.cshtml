@model ATAPS.Models.DisplayObject.EventDateDisplayObject

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<div>
    <h4>EventDate</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Event Name
        </dt>

        <dd>
            @*
                @Html.DisplayFor(model => model.Event.EventName)
            *@
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.EventDate)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.EventDate)
        </dd>
        <dt>
            Agendas
        </dt>

        <dd>
            @foreach(var item in Model.Agendas)
            {
                <p>
                    <a href="/Agenda/Details/@item.ID?filter=@ViewBag.FilterID">
                        @Html.DisplayFor(modelItem => item.AgendaName)
                    </a>
                </p>
            }
        </dd>
        
    </dl>
</div>
<p>
    <a href="/EventDates/Edit/@Model.EventDate.ID?filter=@ViewBag.FilterID">Edit</a> | 
    <a href="/EventDates/?filter=@ViewBag.FilterID">Back to List</a>
</p>
